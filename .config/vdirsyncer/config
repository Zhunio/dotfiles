[general]
status_path = "~/.local/share/vdirsyncer/status/"

[pair icloud]
a = "icloud_remote"
b = "icloud_local"
collections = ["from a"]
conflict_resolution = "a wins"

[storage icloud_local]
type = "filesystem"
path = "~/.local/share/calendars/zhunio-icloud/"
fileext = ".ics"

[storage icloud_remote]
type = "caldav"
url = "https://caldav.icloud.com/"
username.fetch = ["command", "printenv", "VDIRSINCER_USERNAME"]
password.fetch = ["command", "printenv", "VDIRSINCER_PASSWORD"]

