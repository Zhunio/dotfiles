[calendars]

[[Calendar]]
path = ~/.local/share/calendars/zhunio-icloud/*
type = discover
color = dark green

[locale]
timeformat = %I:%M %p
dateformat = %m/%d/%Y
longdateformat = %m/%d/%Y
datetimeformat = %m/%d/%Y %I:%M %p
longdatetimeformat = %m/%d/%Y %I:%M %p

[default]
timedelta = 7d
highlight_event_days = True

[view]
dynamic_days = False

[palette]
header = dark cyan, ''
footer = dark cyan, ''
edit = white, ''
edit focus = black, light magenta

reveal focus = black, dark green
today focus = black, dark green
today = black, light cyan

date header = white, ''
date header focused = black, dark green
date header selected = black, dark green

dayname = dark red, ''
monthname = dark red, ''

